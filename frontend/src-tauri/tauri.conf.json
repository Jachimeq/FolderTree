{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "FolderTreePRO",
    "version": "0.1.0"
  },
"tauri": {
"allowlist": {
  "all": false,
  "shell": {
    "all": false,
    "open": false
  },
  "fs": {
    "all": false
  },
  "path": {
    "all": true
  },
  "dialog": {
    "all": true
  }
},
"updater": {
  "active": true,
  "endpoints": [
    "https://github.com/Jachimeq/FolderTreePRO/releases/latest/download/latest.json"
  ],
  "dialog": true,
  "pubkey": "WSTAW_TU_PUBLIC_KEY"
},
"bundle": {
      "active": true,
      "identifier": "com.jachimeq.foldertreepro",
     "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "FolderTreePRO",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}